{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch Node",
      "program": "${workspaceFolder}/app/build/main.js",
      "preLaunchTask": "rebuild:app",
      "outFiles": ["${workspaceFolder}/app/build/**/*.js"],
      "sourceMaps": true
    },
    {
      "type": "dart",
      "request": "launch",
      "name": "Launch Flutter",
      "program": "${workspaceFolder}/web/lib/main.dart",
      "toolArgs": ["--dart-define", "PORT=3000"]
    }
  ]
}
