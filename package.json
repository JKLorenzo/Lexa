{
  "name": "parallax",
  "version": "1.0.0",
  "description": "A multi-purpose Discord bot for gaming.",
  "main": "build/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:docker": "docker build . -t jklorenzo/parallax",
    "clean": "is-ci || rimraf build",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "prepare": "is-ci || husky install",
    "prettier": "prettier --check **/*.{json,yml,yaml}",
    "prettier:fix": "prettier --write **/*.{json,yml,yaml}",
    "start": "node ."
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKLorenzo/Parallax.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "docker",
    "heroku",
    "discord-bot"
  ],
  "author": "Juruel Keanu Lorenzo <keanulorenzo32@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/JKLorenzo/Parallax/issues"
  },
  "homepage": "https://github.com/JKLorenzo/Parallax#readme",
  "dependencies": {
    "@discordjs/opus": "^0.8.0",
    "@discordjs/voice": "^0.11.0",
    "color": "^4.2.3",
    "common-tags": "^1.8.2",
    "discord.js": "^13.8.0",
    "dotenv": "^16.0.1",
    "ffmpeg-static": "^5.0.2",
    "g-i-s": "^2.1.6",
    "humanize-duration": "^3.27.2",
    "libsodium-wrappers": "^0.7.10",
    "lodash": "^4.17.21",
    "mongodb": "^4.9.0",
    "node-cron": "^3.0.2",
    "node-fetch": "^3.2.10",
    "node-google-tts-api": "^1.1.1",
    "play-dl": "^1.9.5",
    "probe-image-size": "^7.2.3",
    "soundcloud-scraper": "^5.0.3",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/color": "^3.0.3",
    "@types/common-tags": "^1.8.1",
    "@types/humanize-duration": "^3.27.1",
    "@types/lodash": "^4.14.184",
    "@types/node-cron": "^3.0.2",
    "@types/probe-image-size": "^7.2.0",
    "@types/spotify-web-api-node": "^5.0.7",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "husky": "^8.0.1",
    "is-ci": "^3.0.1",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.2"
  }
}
