{
  "name": "lexa",
  "version": "1.0.0",
  "description": "A Discord bot for automation and moderation.",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "is-ci || rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "postinstall": "npm run clean && npm run build",
    "prepare": "is-ci || husky install",
    "prettier": "prettier --check **/*.{json,yml,yaml}",
    "prettier:fix": "prettier --write **/*.{json,yml,yaml}"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKLorenzo/Lexa.git"
  },
  "keywords": [
    "nodejs",
    "discord-bot",
    "lexa-discord"
  ],
  "author": "Juruel Keanu Lorenzo <keanulorenzo32@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/JKLorenzo/Lexa/issues"
  },
  "homepage": "https://github.com/JKLorenzo/Lexa#readme",
  "dependencies": {
    "discord-api-types": "^0.20.2",
    "discord.js": "^13.0.0-dev.30a58dc.1627430624",
    "mongodb": "^4.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^4.28.5",
    "@typescript-eslint/parser": "^4.28.5",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-prettier": "^3.4.0",
    "husky": "^7.0.1",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.1",
    "prettier": "^2.3.2",
    "typescript": "^4.3.5"
  }
}
