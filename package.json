{
  "name": "parallax",
  "version": "1.0.0",
  "description": "A multi-purpose Discord bot for gaming.",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "is-ci || rimraf dist",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "postinstall": "npm run clean && npm run build",
    "prepare": "is-ci || husky install",
    "prettier": "prettier --check **/*.{json,yml,yaml}",
    "prettier:fix": "prettier --write **/*.{json,yml,yaml}"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKLorenzo/Parallax.git"
  },
  "keywords": [
    "nodejs",
    "discord-bot",
    "qg-discord"
  ],
  "author": "Juruel Keanu Lorenzo <keanulorenzo32@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/JKLorenzo/Parallax/issues"
  },
  "homepage": "https://github.com/JKLorenzo/Parallax#readme",
  "dependencies": {
    "@discordjs/opus": "^0.6.0",
    "@discordjs/voice": "^0.6.0",
    "color": "^4.0.1",
    "common-tags": "^1.8.0",
    "discord-api-types": "^0.22.0",
    "discord.js": "^13.1.0",
    "ffmpeg-static": "^4.4.0",
    "g-i-s": "^2.1.6",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mongodb": "^4.1.1",
    "node-cron": "^3.0.0",
    "node-fetch": "^2.6.1",
    "probe-image-size": "^7.2.1",
    "spotify-web-api-node": "^5.0.2",
    "youtube-dl-exec": "^1.2.5",
    "youtube-search": "^1.1.6",
    "ytdl-core": "^4.9.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@types/color": "^3.0.2",
    "@types/common-tags": "^1.8.1",
    "@types/lodash": "^4.14.172",
    "@types/node-cron": "^2.0.4",
    "@types/node-fetch": "^2.5.12",
    "@types/probe-image-size": "^7.0.1",
    "@types/spotify-web-api-node": "^5.0.3",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^3.4.1",
    "husky": "^7.0.2",
    "is-ci": "^3.0.0",
    "lint-staged": "^11.1.2",
    "prettier": "^2.3.2",
    "typescript": "^4.4.2"
  }
}
