{
  "name": "parallax",
  "version": "1.0.0",
  "description": "A multi-purpose Discord bot for gaming.",
  "main": "dist/main.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "clean": "is-ci || rimraf {dist,*.mp3}",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "postinstall": "npm run clean && npm run build",
    "prepare": "is-ci || husky install",
    "prettier": "prettier --check **/*.{json,yml,yaml}",
    "prettier:fix": "prettier --write **/*.{json,yml,yaml}"
  },
  "engines": {
    "node": "16.x"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JKLorenzo/Parallax.git"
  },
  "keywords": [
    "nodejs",
    "discord-bot",
    "qg-discord"
  ],
  "author": "Juruel Keanu Lorenzo <keanulorenzo32@gmail.com>",
  "license": "AGPL-3.0-only",
  "bugs": {
    "url": "https://github.com/JKLorenzo/Parallax/issues"
  },
  "homepage": "https://github.com/JKLorenzo/Parallax#readme",
  "dependencies": {
    "@discordjs/opus": "^0.7.0",
    "@discordjs/voice": "^0.7.5",
    "color": "^4.1.0",
    "common-tags": "^1.8.2",
    "discord-api-types": "^0.25.2",
    "discord.js": "^13.3.1",
    "ffmpeg-static": "^4.4.0",
    "g-i-s": "^2.1.6",
    "libsodium-wrappers": "^0.7.9",
    "lodash": "^4.17.21",
    "mongodb": "^4.2.2",
    "node-cron": "^3.0.0",
    "node-fetch": "^3.1.0",
    "node-google-tts-api": "^1.1.1",
    "play-dl": "^1.6.4",
    "probe-image-size": "^7.2.2",
    "soundcloud-scraper": "^5.0.2",
    "spotify-web-api-node": "^5.0.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^15.0.0",
    "@commitlint/config-conventional": "^15.0.0",
    "@types/color": "^3.0.2",
    "@types/common-tags": "^1.8.1",
    "@types/lodash": "^4.14.178",
    "@types/node-cron": "^3.0.0",
    "@types/node-fetch": "^3.0.3",
    "@types/probe-image-size": "^7.0.1",
    "@types/spotify-web-api-node": "^5.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.0",
    "@typescript-eslint/parser": "^5.8.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.4",
    "is-ci": "^3.0.1",
    "lint-staged": "^12.1.3",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  }
}
